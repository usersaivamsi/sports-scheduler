<%- include('partials/header') %>

<h2>Sessions I Joined</h2>
<table class="table">
<thead><tr><th>Sport</th><th>Date</th><th>Venue</th><th>Creator</th><th>Players</th></tr></thead>
<tbody>
  <% joined.forEach(s => { %>
    <tr>
      <td><%= s.sport ? s.sport.name : '-' %></td>
      <td><%= s.dateTime.toLocaleString() %></td>
      <td><%= s.venue %></td>
      <td><%= s.createdBy ? s.createdBy.name : '-' %></td>
      <td><%= s.players.length %></td>
    </tr>
  <% }) %>
</tbody>
</table>

<%- include('partials/footer') %>
